{
  "name": "fucsnyp",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [ "json", "snippets" ],
        "path": "./snippets/json.json"
      },
      {
        "language": [ "sh", "bash" ],
        "path": "./snippets/sh.json"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.json"
      },
      {
        "language":[ "php", "html", "css" ],
        "path": "./snippets/css.json"
      },
      {
        "language":[ "php", "html", "css" ],
        "path": "./snippets/bootstrapcss.json"
      },
      {
        "language":[ "php", "html", "css" ],
        "path": "./snippets/ajax.json"
      },
      {
        "language":[ "php", "html" ],
        "path": "./snippets/html.json"
      },
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": [ "plaintext", "markdown", "tex", "html", "global" ],
        "path": "./snippets/all.json"
      }
    ]
  }
}
